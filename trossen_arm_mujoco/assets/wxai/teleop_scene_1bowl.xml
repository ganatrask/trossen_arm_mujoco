    <mesh name="ramekin_collision_31" file="Threshold_Ramekin_White_Porcelain/model_collision_31.obj"/>
  </asset>

  <worldbody>
    <!-- Lights for better visibility -->
    <light name="top_light" pos="0 0 1.5" dir="0 0 -1" diffuse="0.8 0.8 0.8" specular="0.3 0.3 0.3" directional="false"/>
    <light name="side_light" pos="0.5 0.5 1.0" dir="-0.5 -0.5 -1" diffuse="0.6 0.6 0.6" specular="0.2 0.2 0.2" directional="false"/>

    <camera name="main_view" pos="1.020 0.160 1.480" xyaxes="-0.155 0.988 0.000 -0.743 -0.116 0.659"/>
    <!-- Floor -->
    <geom name="floor" size="0 0 0.05" type="plane" material="groundplane"/>

    <!-- Table -->
    <geom name="tabletop" type="box" size="0.8 0.8 0.02" pos="0 0 0.02" material="table_mat"/>

    <!-- Camera positioned to see the entire robot -->
    <camera name="cam_high" pos="-0.776 -0.127 -0.337" xyaxes="0.032 0.999 0.000 -0.840 0.027 0.542" mode="fixed" fovy="60"/>
    <camera name="cam_front" pos="-1.339 -0.254 0.229" xyaxes="-0.021 1.000 0.000 -0.625 -0.013 0.781" mode="fixed" fovy="60"/>

    <!-- Container -->
    <body name="container" pos="-0.63 -0.15 0.04" quat="0.7071 0 0 0.7071">
      <geom name="container_bottom" type="box" size="0.1625 0.1325 0.0025" pos="0 0 0.0025" material="container_mat" contype="1" conaffinity="1"/>
      <geom name="container_front" type="box" size="0.1625 0.0025 0.0508" pos="0 -0.13 0.0558" material="container_mat" contype="1" conaffinity="1"/>
      <geom name="container_back" type="box" size="0.1625 0.0025 0.0508" pos="0 0.13 0.0558" material="container_mat" contype="1" conaffinity="1"/>
      <geom name="container_left" type="box" size="0.0025 0.1325 0.0508" pos="-0.16 0 0.0558" material="container_mat" contype="1" conaffinity="1"/>
      <geom name="container_right" type="box" size="0.0025 0.1325 0.0508" pos="0.16 0 0.0558" material="container_mat" contype="1" conaffinity="1"/>
    </body>

    <!-- Bowl 1 - single bowl for simplest scene -->
    <body name="bowl_1" pos="-0.22 -0.26 0.04">
      <geom type="mesh" mesh="threshold_ramekin_mesh" material="threshold_ramekin_mat" contype="0" conaffinity="0" group="2"/>
      <geom type="mesh" mesh="ramekin_collision_0" contype="1" conaffinity="1" friction="1 0.005 0.0001" rgba="0 0 0 0"/>
      <geom type="mesh" mesh="ramekin_collision_1" contype="1" conaffinity="1" friction="1 0.005 0.0001" rgba="0 0 0 0"/>
      <geom type="mesh" mesh="ramekin_collision_2" contype="1" conaffinity="1" friction="1 0.005 0.0001" rgba="0 0 0 0"/>
      <geom type="mesh" mesh="ramekin_collision_3" contype="1" conaffinity="1" friction="1 0.005 0.0001" rgba="0 0 0 0"/>
      <geom type="mesh" mesh="ramekin_collision_4" contype="1" conaffinity="1" friction="1 0.005 0.0001" rgba="0 0 0 0"/>
      <geom type="mesh" mesh="ramekin_collision_5" contype="1" conaffinity="1" friction="1 0.005 0.0001" rgba="0 0 0 0"/>
      <geom type="mesh" mesh="ramekin_collision_6" contype="1" conaffinity="1" friction="1 0.005 0.0001" rgba="0 0 0 0"/>
      <geom type="mesh" mesh="ramekin_collision_7" contype="1" conaffinity="1" friction="1 0.005 0.0001" rgba="0 0 0 0"/>
      <geom type="mesh" mesh="ramekin_collision_8" contype="1" conaffinity="1" friction="1 0.005 0.0001" rgba="0 0 0 0"/>
      <geom type="mesh" mesh="ramekin_collision_9" contype="1" conaffinity="1" friction="1 0.005 0.0001" rgba="0 0 0 0"/>
      <geom type="mesh" mesh="ramekin_collision_10" contype="1" conaffinity="1" friction="1 0.005 0.0001" rgba="0 0 0 0"/>
      <geom type="mesh" mesh="ramekin_collision_11" contype="1" conaffinity="1" friction="1 0.005 0.0001" rgba="0 0 0 0"/>
      <geom type="mesh" mesh="ramekin_collision_12" contype="1" conaffinity="1" friction="1 0.005 0.0001" rgba="0 0 0 0"/>
      <geom type="mesh" mesh="ramekin_collision_13" contype="1" conaffinity="1" friction="1 0.005 0.0001" rgba="0 0 0 0"/>
      <geom type="mesh" mesh="ramekin_collision_14" contype="1" conaffinity="1" friction="1 0.005 0.0001" rgba="0 0 0 0"/>
      <geom type="mesh" mesh="ramekin_collision_15" contype="1" conaffinity="1" friction="1 0.005 0.0001" rgba="0 0 0 0"/>
      <geom type="mesh" mesh="ramekin_collision_16" contype="1" conaffinity="1" friction="1 0.005 0.0001" rgba="0 0 0 0"/>
      <geom type="mesh" mesh="ramekin_collision_17" contype="1" conaffinity="1" friction="1 0.005 0.0001" rgba="0 0 0 0"/>
      <geom type="mesh" mesh="ramekin_collision_18" contype="1" conaffinity="1" friction="1 0.005 0.0001" rgba="0 0 0 0"/>
