<mujoco model="wxai base scene with table - 1 bowl">
  <include file="../food_task/teleop_follower_spoon.xml"/>

  <compiler meshdir="../meshes" texturedir="../"/>

  <statistic center="0 0 0.3" extent="0.8"/>

  <visual>
    <headlight diffuse="0.6 0.65 0.75" ambient="0.5 0.5 0.6" specular="0.1 0.1 0.1"/>
    <rgba haze="0.15 0.25 0.35 1"/>
    <global offwidth="1280" offheight="720"/>
    <quality shadowsize="4096"/>
  </visual>

  <asset>
    <texture type="skybox" builtin="gradient" rgb1="0.9725 0.9608 0.8706" rgb2="0.5 0.5 0.5" width="512" height="3072"/>
    <texture type="2d" name="groundplane" builtin="checker" mark="edge" rgb1="0.2 0.3 0.4" rgb2="0.1 0.2 0.3"
      markrgb="0.8 0.8 0.8" width="300" height="300"/>
    <material name="groundplane" texture="groundplane" texuniform="true" texrepeat="5 5" reflectance="0.2"/>

    <texture name="table_tex" type="cube" builtin="flat" mark="cross" width="127" height="762"
      rgb1="0.6 0.5 0.4" rgb2="0.6 0.5 0.4" markrgb="1 1 1" random="0.01"/>
    <material name="table_mat" texture="table_tex" texuniform="true" rgba="0.8 0.7 0.6 1"/>

    <!-- Container material (plastic-like) -->
    <material name="container_mat" rgba="0.3 0.3 0.35 1" specular="0.3" shininess="0.3"/>

    <!-- Threshold Ramekin White Porcelain -->
    <texture name="threshold_ramekin_texture" type="2d"
             file="meshes/Threshold_Ramekin_White_Porcelain/texture.png"/>
    <material name="threshold_ramekin_mat" texture="threshold_ramekin_texture" specular="0.5" shininess="0.5"/>
    <mesh name="threshold_ramekin_mesh" file="Threshold_Ramekin_White_Porcelain/model.obj"/>

    <!-- Collision meshes for Threshold Ramekin -->
    <mesh name="ramekin_collision_0" file="Threshold_Ramekin_White_Porcelain/model_collision_0.obj"/>
    <mesh name="ramekin_collision_1" file="Threshold_Ramekin_White_Porcelain/model_collision_1.obj"/>
    <mesh name="ramekin_collision_2" file="Threshold_Ramekin_White_Porcelain/model_collision_2.obj"/>
    <mesh name="ramekin_collision_3" file="Threshold_Ramekin_White_Porcelain/model_collision_3.obj"/>
    <mesh name="ramekin_collision_4" file="Threshold_Ramekin_White_Porcelain/model_collision_4.obj"/>
    <mesh name="ramekin_collision_5" file="Threshold_Ramekin_White_Porcelain/model_collision_5.obj"/>
    <mesh name="ramekin_collision_6" file="Threshold_Ramekin_White_Porcelain/model_collision_6.obj"/>
    <mesh name="ramekin_collision_7" file="Threshold_Ramekin_White_Porcelain/model_collision_7.obj"/>
    <mesh name="ramekin_collision_8" file="Threshold_Ramekin_White_Porcelain/model_collision_8.obj"/>
    <mesh name="ramekin_collision_9" file="Threshold_Ramekin_White_Porcelain/model_collision_9.obj"/>
    <mesh name="ramekin_collision_10" file="Threshold_Ramekin_White_Porcelain/model_collision_10.obj"/>
    <mesh name="ramekin_collision_11" file="Threshold_Ramekin_White_Porcelain/model_collision_11.obj"/>
    <mesh name="ramekin_collision_12" file="Threshold_Ramekin_White_Porcelain/model_collision_12.obj"/>
    <mesh name="ramekin_collision_13" file="Threshold_Ramekin_White_Porcelain/model_collision_13.obj"/>
    <mesh name="ramekin_collision_14" file="Threshold_Ramekin_White_Porcelain/model_collision_14.obj"/>
    <mesh name="ramekin_collision_15" file="Threshold_Ramekin_White_Porcelain/model_collision_15.obj"/>
    <mesh name="ramekin_collision_16" file="Threshold_Ramekin_White_Porcelain/model_collision_16.obj"/>
    <mesh name="ramekin_collision_17" file="Threshold_Ramekin_White_Porcelain/model_collision_17.obj"/>
    <mesh name="ramekin_collision_18" file="Threshold_Ramekin_White_Porcelain/model_collision_18.obj"/>
    <mesh name="ramekin_collision_19" file="Threshold_Ramekin_White_Porcelain/model_collision_19.obj"/>
    <mesh name="ramekin_collision_20" file="Threshold_Ramekin_White_Porcelain/model_collision_20.obj"/>
    <mesh name="ramekin_collision_21" file="Threshold_Ramekin_White_Porcelain/model_collision_21.obj"/>
    <mesh name="ramekin_collision_22" file="Threshold_Ramekin_White_Porcelain/model_collision_22.obj"/>
    <mesh name="ramekin_collision_23" file="Threshold_Ramekin_White_Porcelain/model_collision_23.obj"/>
    <mesh name="ramekin_collision_24" file="Threshold_Ramekin_White_Porcelain/model_collision_24.obj"/>
    <mesh name="ramekin_collision_25" file="Threshold_Ramekin_White_Porcelain/model_collision_25.obj"/>
    <mesh name="ramekin_collision_26" file="Threshold_Ramekin_White_Porcelain/model_collision_26.obj"/>
    <mesh name="ramekin_collision_27" file="Threshold_Ramekin_White_Porcelain/model_collision_27.obj"/>
