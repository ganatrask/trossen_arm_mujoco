<mujoco model="kitchen_bowl">

  <option timestep="0.002" iterations="50" solver="Newton" tolerance="1e-10">
    <flag contact="enable" gravity="enable"/>
  </option>

  <!-- 1. Procedural bowl using the SDF bowl plugin -->
  <extension>
    <plugin plugin="mujoco.sdf.bowl">
      <instance name="bowl_shape">
        <!-- tune these for size -->
        <config key="height"    value="0.05"/>   <!-- 5 cm tall -->
        <config key="radius"    value="0.08"/>   <!-- 8 cm radius -->
        <config key="thickness" value="0.002"/>  <!-- 2 mm thick -->
      </instance>
      <instance name="bowl_shape_small">
        <config key="height"    value="0.035"/>
        <config key="radius"    value="0.055"/>
        <config key="thickness" value="0.002"/>
      </instance>
      <instance name="bowl_shape_large">
        <config key="height"    value="0.07"/>
        <config key="radius"    value="0.11"/>
        <config key="thickness" value="0.003"/>
      </instance>
    </plugin>
  </extension>

  <asset>
    <!-- mesh generated from the plugin -->
    <mesh name="bowl_mesh">
      <plugin instance="bowl_shape"/>
    </mesh>
    <mesh name="bowl_mesh_small">
      <plugin instance="bowl_shape_small"/>
    </mesh>
    <mesh name="bowl_mesh_large">
      <plugin instance="bowl_shape_large"/>
    </mesh>

    <!-- Plate asset from scanned objects -->
    <texture name="plate_texture" type="2d"
             file="assets/meshes/plate/texture.png"/>
    <material name="plate_mat" texture="plate_texture" specular="0.5" shininess="0.5"/>
    <mesh name="plate_mesh"
          file="assets/meshes/plate/model.obj"/>

    <!-- Table spoon mesh -->
    <mesh name="spoon_mesh" file="assets/meshes/table_spoon.stl" scale="0.001 0.001 0.001"/>
    <material name="spoon_mat" rgba="0.8 0.8 0.85 1" specular="0.7" shininess="0.6"/>

    <!-- Bradshaw plastic bowl from scanned objects -->
    <texture name="bradshaw_bowl_texture" type="2d"
             file="assets/meshes/Bradshaw_International_11642_7_Qt_MP_Plastic_Bowl/texture.png"/>
    <material name="bradshaw_bowl_mat" texture="bradshaw_bowl_texture" specular="0.5" shininess="0.5"/>
